SERVICE_NAME=uasp_streaming_aggregate
PARALLELISM=8
MAIN_CLASS=ru.vtb.uasp.aggregate.UaspStreamingAggregate
PROGRAMARGS="--app.service.name ${SERVICE_NAME} "`
 `"--state.checkpoints.dir hdfs://d5uasp-hdfs.corp.dev.vtb:9000/flink/flink-checkpoints/uasp-streaming-aggregate "`
 `"--app.name.state.day.hour.aggregates DayHourAggregates "`
 `"--max.parallelism 8 "`
 `"--stream.checkpoint.time.seconds 60000 "`
 `"--stream.watermark.time.seconds 60 "`
 `"--stream.window.time.milliseconds  604800000 "`
 `"--consumer.topic.name ift_ivr__uasp_realtime__mdm_enrichment__uaspdto "`
 `"--producer.topic.name ift_ivr__uasp_realtime__aggregate__uaspdto "`
 `"--producer.dlq.topic.name ift_ivr__uasp_realtime__aggregate__dlq "`
 `"--producer.filter.status.topic.name ift_ivr__uasp_realtime__aggregate__filter "`
 `"--bootstrap.servers ${BOOTSTRAP_SERVERS} "`
 `"--group.id uasp-streaming-aggregate-itest "`
 `"--security.protocol SSL "`
 `"--ssl.truststore.location ${TRUSTSTORE_LOCATION} "`
 `"--ssl.truststore.password ${KAFKA_SSL_TRUSTSTORE_PASSWORD} "`
 `"--ssl.keystore.location ${KEYSTORE_LOCATION} "`
 `"--ssl.keystore.password ${KAFKA_SSL_KEYSTORE_PASSWORD} "`
 `"--ssl.key.password ${KAFKA_SSL_KEY_PASSWORD} "`
 `"--key.deserializer org.apache.kafka.common.serialization.StringDeserializer "`
 `"--value.deserializer org.apache.kafka.common.serialization.ByteArrayDeserializer "`
 `"--key.serializer org.apache.kafka.common.serialization.ByteArraySerializer "`
 `"--value.serializer org.apache.kafka.common.serialization.ByteArraySerializer "`
 `"--ssl.truststore.type PKCS12 "`
 `"--ssl.keystore.type PKCS12 "`
 `"--transaction.timeout.ms 1200000 "`
 `"--max.block.ms 90000 "`
 `"--enable.auto.commit true "`
 `"--enable.idempotence true "`
 `"--max.in.flight.requests.per.connection 5 "`
  `"--retries 1 "`
  `"--acks all "`
  `"--isolation.level read_committed "`
  `"--transaction.max.timeout.ms 1200000 "