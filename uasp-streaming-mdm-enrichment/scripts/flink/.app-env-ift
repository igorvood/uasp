SERVICE_NAME=mdm_enrichment
PARALLELISM=8
MAIN_CLASS=ru.vtb.uasp.mdm.enrichment.EnrichmentJob
PROGRAMARGS="--app.service.name ${SERVICE_NAME} "`
 `"--max.parallelism 8 "`
 `"--state.checkpoints.dir hdfs://d5uasp-hdfs.corp.dev.vtb:9000/flink/flink-checkpoints/mdm_enrichment "`
 `"--stream.checkpoint.time.seconds 120000 "`
 `"--stream.watermark.time.seconds 60 "`
 `"--stream.window.time.milliseconds  604800000 "`
 `"--app.ref.topic.name ift_ivr__uasp_realtime__input_converter__mdm_cross_link__uaspdto "`
 `"--app.ref.status.topic.name ift_ivr__uasp_realtime__mdm_enrichment__mdm_cross_link__status "`
 `"--app.main.topic.name ift_ivr__uasp_realtime__input_converter__way4_issuing_operation__uaspdto "`
 `"--app.dlq.topic.name ift_ivr__uasp_realtime__mdm_enrichment__for_additional_enrichment__uaspdto "`
 `"--app.output.topic.name ift_ivr__uasp_realtime__mdm_enrichment__uaspdto "`
 `"--app.avro.schema.topic.name streaming_avro-schemas "`
 `"--bootstrap.servers ${BOOTSTRAP_SERVERS} "`
 `"--auto.offset.reset latest "`
 `"--isolation.level read_committed "`
 `"--enable.idempotence true "`
# `"--max.in.flight.requests.per.connection 5 "`
# `"--offsets.commit.timeout.ms 10000 "`
 `"--retries 1 "`
 `"--acks all "`
# `"--session.timeout.ms 120000 "`
# `"--transaction.max.timeout.ms 120000 "`
 `"--transaction.timeout.ms 30000 "`
 `"--group.id mdm_enrichment_avl "`
 `"--security.protocol SSL "`
 `"--ssl.truststore.location ${TRUSTSTORE_LOCATION} "`
 `"--ssl.truststore.password ${KAFKA_SSL_TRUSTSTORE_PASSWORD} "`
 `"--ssl.keystore.location ${KEYSTORE_LOCATION} "`
 `"--ssl.keystore.password ${KAFKA_SSL_KEYSTORE_PASSWORD} "`
 `"--ssl.key.password ${KAFKA_SSL_KEY_PASSWORD} "`
 `"--key.deserializer org.apache.kafka.common.serialization.StringDeserializer "`
 `"--value.deserializer org.apache.kafka.common.serialization.ByteArrayDeserializer "`
 `"--key.serializer org.apache.kafka.common.serialization.ByteArraySerializer "`
 `"--value.serializer org.apache.kafka.common.serialization.ByteArraySerializer "`
 `"--ssl.truststore.type PKCS12 "`
 `"--ssl.keystore.type PKCS12 "`
 `"--transaction.timeout.ms 900000 "`
 `"--max.block.ms 90000 "`
 `"--enable.auto.commit true "
